<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".User.UserWishlist">
      <SearchView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_margin="16sp"
          android:iconifiedByDefault="false"
          android:queryHint="Search product name"
          android:background="@drawable/serch"
          android:id="@+id/svUserhomefargmentrsearchproduct"
          />


      <androidx.recyclerview.widget.RecyclerView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:id="@+id/rvUserwishlistrecyclerlist"
          android:layout_marginTop="16sp"
          android:layout_below="@id/svUserhomefargmentrsearchproduct"
          />
</RelativeLayout>